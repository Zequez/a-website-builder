{
  "name": "a-website-builder",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node ./dist/ts/server/index.js",
    "dev": "pnpm dev:server",
    "dev:app": "vite",
    "dev:server": "NODE_ENV=development tsx watch ./server/index.ts",
    "build": "concurrently pnpm:build:app pnpm:build:server",
    "build:app": "NODE_ENV=production vite build",
    "build:server": "NODE_ENV=production tsc --project tsconfig.json && tsc-alias -p tsconfig.json && pnpm build:server:copysql",
    "build:server:copysql": "node ./scripts/copySql.js",
    "deploy:prod": "vercel deploy --prod",
    "db:migrate": "tsx ./scripts/migrate.ts",
    "test": "concurrently pnpm:test:server pnpm:test:runner",
    "test:runner": "SILENCE_SQL_LOGS=true NODE_ENV=test vitest",
    "test:server": "SILENCE_SQL_LOGS=true NODE_ENV=test tsx watch ./server/index.ts"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "pnpm": {
    "allowedDeprecatedVersions": {
      "debug": "4.1.1",
      "uuid": "3.3.2",
      "fsevents": "2.1.3"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/core"
      ]
    }
  },
  "dependencies": {
    "@iconify-json/fa6-solid": "^1.1.20",
    "@iconify/icons-fa-solid": "^1.2.4",
    "@preact/preset-vite": "^2.8.1",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.21",
    "@types/pg": "^8.11.2",
    "@types/uuid": "^9.0.8",
    "@unocss/preset-uno": "^0.58.5",
    "@unocss/reset": "^0.58.5",
    "@vercel/node": "^3.0.20",
    "@vercel/postgres": "^0.7.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "classnames": "^2.5.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "elm": "0.19.1-6",
    "express": "^4.18.3",
    "js-base64": "^3.7.7",
    "js-sha256": "^0.11.0",
    "jsonwebtoken": "^9.0.2",
    "mime": "^4.0.1",
    "node-html-parser": "^6.1.12",
    "pg": "^8.11.3",
    "preact": "github:zequez/preact#main",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "unocss": "^0.58.5",
    "unplugin-icons": "^0.18.5",
    "uuid": "^9.0.1",
    "vercel": "^33.5.2",
    "vite": "^5.1.4",
    "vite-plugin-elm": "^3.0.0",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1",
    "wait-port": "^1.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17"
  }
}
