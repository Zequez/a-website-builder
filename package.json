{
  "name": "a-website-builder",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node ./dist/ts/server/index.js",
    "dev": "NODE_ENV=development tsx watch ./server/index.ts",
    "dev:app": "vite",
    "dev:server": "NODE_ENV=development tsx watch ./server/index.ts",
    "build": "NODE_ENV=production  pnpm build:app && pnpm build:server",
    "build:app": "NODE_ENV=production vite build",
    "build:server": "NODE_ENV=production tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "deploy:prod": "vercel deploy --prod",
    "db:migrations:add": "tsx ./scripts/createMigration.ts",
    "db:migrations:run": "tsx ./scripts/migrate.ts",
    "db:migrations:run:prod": "NODE_ENV=production tsx ./scripts/migrate.ts",
    "db:seeds": "tsx ./scripts/applySeeds.ts",
    "test": "pnpm test:runner",
    "test:app": "NODE_ENV=test vitest -c vitest.config.ts -r app/",
    "test:runner": "SILENCE_SQL_LOGS=true NODE_ENV=test vitest --no-file-parallelism --disable-console-intercept -r server/ -c vitest.config.ts",
    "test:server": "SILENCE_SQL_LOGS=true NODE_ENV=test tsx watch ./server/index.ts"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "pnpm": {
    "allowedDeprecatedVersions": {
      "debug": "4.1.1",
      "uuid": "3.3.2",
      "fsevents": "2.1.3"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/core"
      ]
    }
  },
  "dependencies": {
    "@codemirror/commands": "^6.3.3",
    "@codemirror/lang-css": "^6.2.1",
    "@codemirror/lang-html": "^6.4.8",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-yaml": "^6.0.0",
    "@codemirror/language": "^6.10.1",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.26.0",
    "@iconify-json/fa6-solid": "^1.1.20",
    "@lezer/common": "^1.2.1",
    "@preact/preset-vite": "^2.8.1",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.21",
    "@types/pg": "^8.11.2",
    "@types/uuid": "^9.0.8",
    "@unocss/core": "^0.58.6",
    "@unocss/preset-mini": "^0.58.6",
    "@unocss/preset-uno": "^0.58.5",
    "@unocss/reset": "^0.58.5",
    "@vercel/node": "^3.0.20",
    "@vercel/postgres": "^0.7.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "classnames": "^2.5.1",
    "codemirror": "^6.0.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "js-base64": "^3.7.7",
    "js-sha256": "^0.11.0",
